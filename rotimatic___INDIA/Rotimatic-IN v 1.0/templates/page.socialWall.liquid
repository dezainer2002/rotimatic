{%-liquid

  assign cssFile      =     'socialWall.css' | asset_url
  assign packeryFile  =     'https://unpkg.com/packery@2/dist/packery.pkgd.js'
  assign jsFile       =     'socialWall.js' | asset_url

  echo '<link rel="stylesheet" href="' | append: cssFile | append: '">'

  section 'socialWall__sec'

  echo '<script src="' | append: packeryFile | append: '"></script>'
  echo '<script src="' | append: jsFile | append: '"></script>'

-%}

