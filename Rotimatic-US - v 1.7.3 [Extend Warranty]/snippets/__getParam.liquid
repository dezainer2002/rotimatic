{%-liquid
  if paramStart != blank
    if content_for_header contains 'script id="__st"'
      capture pppppp____
        assign p______    =   content_for_header | split: 'script id="__st"'
        assign p______p   =   p______[1] | split: 'script'

        assign p___00     =   p______p[0]

        assign getRefined___01    =    p___00 | split: '"pageurl":"'

        assign getRefined___02    =    getRefined___01[1] | split: '"'

        assign getRefined___03    =    getRefined___02[0] | split: paramStart

        if getRefined___03[1] contains '\u0026'

          assign getRefined___04    =    getRefined___03[1] | split: '\u0026'

          echo getRefined___04[0]

        else

          echo getRefined___03[1]

        endif

      endcapture

      if pppppp____ != blank
        echo pppppp____
      endif

    endif

  endif

  comment
    render '__getParam' with { paramStart: 'second=' }
  endcomment
-%}